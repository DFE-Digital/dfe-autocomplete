{
  "name": "dfe-autocomplete",
  "version": "0.0.1",
  "description": "DfE Autocomplete built on top of accessible autocomplete lib",
  "main": "dist/dfe-autocomplete.min.js",
  "style": "dist/dfe-autocomplete.min.css",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "test",
    "build:css": "node-sass src/dfe-autocomplete.scss -o dist/ && csso dist/dfe-autocomplete.css -o dist/dfe-autocomplete.min.css",
    "build:js": "cross-env NODE_ENV=production webpack --progress --display-modules",
    "build": "npm run -s 'build:js' 'build:css'",
    "standard": "standard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DFE-Digital/dfe-autocomplete.git"
  },
  "author": "DfE-Digital",
  "license": "MIT",
  "keywords": [
    "a11y",
    "accessibility",
    "autocomplete",
    "component",
    "plugin",
    "typeahead",
    "widget"
  ],
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/plugin-transform-member-expression-literals": "^7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.1.0",
    "@babel/plugin-transform-property-literals": "^7.0.0",
    "@babel/plugin-transform-react-jsx": "^7.0.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/register": "^7.0.0",
    "babel-loader": "^8.2.3",
    "copy-webpack-plugin": "^6.4.1",
    "cross-env": "^5.2.0",
    "css-loader": "^6.7.1",
    "csso-cli": "^3.0.0",
    "node-sass": "^7.0.1",
    "sass": "^1.49.11",
    "sass-loader": "^12.6.0",
    "source-map-loader": "^1.1.3",
    "style-loader": "^3.3.1",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^5.71.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.3"
  },
  "bugs": {
    "url": "https://github.com/DFE-Digital/dfe-autocomplete/issues"
  },
  "homepage": "https://github.com/DFE-Digital/dfe-autocomplete#readme",
  "standard": {
    "parser": "babel-eslint"
  },
  "dependencies": {
    "accessible-autocomplete": "^2.0.4",
    "govuk-frontend": "^3.14.0"
  }
}
